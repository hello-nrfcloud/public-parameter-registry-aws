{
  "name": "@nordicsemiconductor/public-parameter-registry",
  "version": "0.0.0-development",
  "description": "Public parameter registry developed using AWS CDK in TypeScript",
  "type": "module",
  "scripts": {
    "test": "tsx --no-warnings --test ./lambda/*.spec.ts",
    "test:e2e": "$(set -o pipefail && npx tsx ./feature-runner/run-features.ts | npx tsx ./feature-runner/console-reporter.ts)",
    "prepare": "husky install && check-node-version --package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hello-nrfcloud/public-parameter-registry-aws-js.git"
  },
  "bugs": {
    "url": "https://github.com/hello-nrfcloud/public-parameter-registry-aws-js/issues"
  },
  "homepage": "https://github.com/hello-nrfcloud/public-parameter-registry-aws-js#readme",
  "keywords": [
    "nordicsemiconductor",
    "cellular-iot",
    "aws",
    "cdk"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@aws-sdk/client-cloudfront": "3.462.0",
    "@aws-sdk/client-iam": "3.462.0",
    "@aws-sdk/client-s3": "3.462.0",
    "@aws-sdk/client-ssm": "3.462.0",
    "@commitlint/config-conventional": "18.4.3",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "13.1.0",
    "@nordicsemiconductor/bdd-markdown": "7.0.15",
    "@nordicsemiconductor/cloudformation-helpers": "9.0.2",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "17.0.0",
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.100",
    "@types/aws-lambda": "8.10.130",
    "@types/glob": "8.1.0",
    "@types/yazl": "2.4.5",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "aws-cdk": "2.111.0",
    "aws-cdk-lib": "2.111.0",
    "cdk": "2.111.0",
    "chalk": "5.3.0",
    "check-node-version": "4.2.1",
    "constructs": "10.3.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-unicorn": "49.0.0",
    "glob": "10.3.10",
    "husky": "8.0.3",
    "prettier": "3.1.0",
    "tsmatchers": "5.0.2",
    "tsx": "4.6.1",
    "typescript": "5.3.2",
    "yazl": "2.5.1"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=20",
    "npm": ">=9"
  },
  "volta": {
    "node": "20.10.0"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc"
}
