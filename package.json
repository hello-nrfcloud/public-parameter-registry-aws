{
  "name": "@nordicsemiconductor/public-parameter-registry",
  "version": "0.0.0-development",
  "description": "Public parameter registry developed using AWS CDK in TypeScript",
  "type": "module",
  "scripts": {
    "test": "tsx --no-warnings --test ./lambda/*.spec.ts",
    "test:e2e": "$(set -o pipefail && npx tsx ./feature-runner/run-features.ts | npx tsx ./feature-runner/console-reporter.ts)",
    "prepare": "husky && check-node-version --package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hello-nrfcloud/public-parameter-registry-aws-js.git"
  },
  "bugs": {
    "url": "https://github.com/hello-nrfcloud/public-parameter-registry-aws-js/issues"
  },
  "homepage": "https://github.com/hello-nrfcloud/public-parameter-registry-aws-js#readme",
  "keywords": [
    "nordicsemiconductor",
    "cellular-iot",
    "aws",
    "cdk"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@aws-sdk/client-cloudfront": "3.600.0",
    "@aws-sdk/client-iam": "3.600.0",
    "@aws-sdk/client-s3": "3.600.0",
    "@aws-sdk/client-ssm": "3.600.0",
    "@bifravst/aws-cdk-lambda-helpers": "1.8.11",
    "@bifravst/ci": "2.0.01",
    "@bifravst/eslint-config-typescript": "6.1.5",
    "@bifravst/prettier-config": "1.0.3",
    "@commitlint/config-conventional": "19.2.2",
    "@nordicsemiconductor/bdd-markdown": "8.2.2",
    "@nordicsemiconductor/cloudformation-helpers": "9.1.0",
    "@types/aws-lambda": "8.10.140",
    "aws-cdk": "2.147.0",
    "aws-cdk-lib": "2.147.0",
    "cdk": "2.147.0",
    "chalk": "5.3.0",
    "check-node-version": "4.2.1",
    "constructs": "10.3.0",
    "husky": "9.0.11",
    "p-retry": "6.2.0",
    "tsmatchers": "5.0.2",
    "tsx": "4.15.7"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "volta": {
    "node": "20.14.0"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "prettier": "@bifravst/prettier-config"
}
